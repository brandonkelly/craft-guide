{
    "name": "wbrowar/guide",
    "description": "A CMS Guide for Craft CMS.",
    "type": "craft-plugin",
    "version": "3.1.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "guide"
    ],
    "support": {
        "docs": "https://github.com/wbrowar/craft-guide/blob/master/README.md",
        "issues": "https://github.com/wbrowar/craft-guide/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Will Browar",
            "homepage": "https://wbrowar.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "craftcms/cms": "^3.7.0"
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
          "wbrowar\\guide\\": "src/"
        }
    },
    "extra": {
        "name": "Guide",
        "handle": "guide",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/wbrowar/craft-guide/master/CHANGELOG.md",
        "class": "wbrowar\\guide\\Guide"
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
